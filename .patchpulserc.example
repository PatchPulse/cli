{
  "$schema": "https://unpkg.com/patch-pulse@latest/schemas/patchpulse-config.schema.json",
  "skip": [
    // Exact package names
    "lodash",
    "chalk",

    // Glob patterns (use * for wildcards)
    "@types/*",
    "test-*",
    "*-dev",

    // Regex patterns (use regex syntax)
    ".*-dev",
    "^@angular/.*",
    "zone\\.js"
  ]
}